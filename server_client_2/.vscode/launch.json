	"version": "0.2.0",
	"configurations": [
		{
			"name": "windows current file",
			"type": "cppvsdbg",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}_x86_pe_debug.exe",
			"args": [
				"password",
				"flag"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"console": "integratedTerminal"
		},
		{
			"name": "windows server2",
			"type": "cppvsdbg",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}\\bin\\server2_x86_pe_debug.exe",
			"args": [
				"password",
				"flag"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"console": "integratedTerminal"
		},
		{
			"name": "windows client2",
			"type": "cppvsdbg",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}\\bin\\client2_x86_pe_debug.exe",
			"args": [
				"password",
				"flag"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"console": "integratedTerminal"
		},
		{
			"name": "linux current file",
			"type": "cppdbg",
			"preLaunchTask": "rebuild",
			"request": "launch",
			"program": "${workspaceRoot}/bin/${fileBasenameNoExtension}",
			"args": [
				"password",
				"flag"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
			]
		},
		{
			"name": "linux server",
			"type": "cppdbg",
			"preLaunchTask": "rebuild",
			"request": "launch",
			"program": "${workspaceRoot}/bin/server2",
			"args": [
				"password",
				"flag"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
			]
		},
		{
			"name": "linux client",
			"type": "cppdbg",
			"preLaunchTask": "rebuild",
			"request": "launch",
			"program": "${workspaceRoot}/bin/client2",
			"args": [
				"password",
				"flag"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
			]
		},
	]
}