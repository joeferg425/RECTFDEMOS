{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "windows",
			"type": "cppvsdbg",
			"request": "launch",
			"preLaunchTask": "rebuild",
			"program": "${workspaceFolder}\\bin\\${workspaceFolderBasename}_x86_pe_debug.exe",
			"args": [
				"L324\\$@#LL@#\\$Ld2LL\\$23\\$5d"
				// "wow, that's a pretty long string for a flag man...and there's not even any special characters or anything"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"console": "integratedTerminal"
		},
		{
			"name": "linux",
			"type": "cppdbg",
			"preLaunchTask": "rebuild",
			"request": "launch",
			"program": "${workspaceRoot}/bin/${workspaceFolderBasename}_x86_elf_debug",
			"args": [
				"L324\\$@#LL@#\\$Ld2LL\\$23\\$5d"
				// "wow, that's a pretty long string for a flag man...and there's not even any special characters or anything"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
			]
		},
	]
}